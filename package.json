{
  "name": "solx402-mcp-server",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:mcp": "pnpm --filter ./apps/mcp-server run dev",
    "dev:web": "pnpm --filter ./apps/web run dev",

    "build:mcp": "pnpm --filter ./apps/mcp-server run build",
    "build:web": "pnpm --filter ./apps/web run build",

    "start:mcp": "pnpm --filter ./apps/mcp-server run start",
    "start:web": "pnpm --filter ./apps/web run start"
  }
}
